{
    "plugins": [
        "plugins/markdown"
    ],
    "recurseDepth": 10,
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "((^|\\/)_|\\/tests?\\/)"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true,
        "includeDate": false,
        "useLongnameInNav": true,
        "showAccessFilter": false,
        "systemColor": "#6d1010",
        "systemName": "thingengine-core",
        "systemSummary": "A Modular, Powerful Virtual Assistant Engine"
    }
}
