{
    "plugins": [
        "plugins/markdown"
    ],
    "recurseDepth": 10,
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "((^|\\/)_|\\/tests?\\/)"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true,
        "includeDate": false,
        "useLongnameInNav": true,
        "copyright": "2019 The Board of Trustees of the Leland Stanford Junior University",
        "inverseNav": true,
        "navType": "vertical",
        "theme": "lumen"
    }
}
